{
  "name": "@plataforma-agenda/core",
  "version": "1.0.0",
  "description": "SDK compartilhado para a plataforma de agendamento",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./database": {
      "import": "./dist/database/index.mjs",
      "require": "./dist/database/index.js",
      "types": "./dist/database/index.d.ts"
    },
    "./types": {
      "import": "./dist/types/index.mjs",
      "require": "./dist/types/index.js",
      "types": "./dist/types/index.d.ts"
    },
    "./validations": {
      "import": "./dist/validations/index.mjs",
      "require": "./dist/validations/index.js",
      "types": "./dist/validations/index.d.ts"
    },
    "./utils": {
      "import": "./dist/utils/index.mjs",
      "require": "./dist/utils/index.js",
      "types": "./dist/utils/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx",
    "test": "vitest",
    "test:unit": "vitest run"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.38.0",
    "zod": "^3.22.4",
    "date-fns": "^2.30.0",
    "date-fns-tz": "^2.0.0",
    "nanoid": "^5.0.4",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.8.0",
    "eslint": "^8.51.0",
    "tsup": "^7.2.0",
    "typescript": "^5.2.0",
    "vitest": "^0.34.0"
  },
  "peerDependencies": {
    "typescript": ">=5.0.0"
  }
}
